package view.dialogs;

import javax.swing.JButton;
import javax.swing.JCheckBox;
import javax.swing.JDialog;
import javax.swing.JTextField;

public abstract class AbstractPathDialog extends JDialog {
	
	public abstract JTextField getPathTextField();

	public abstract JCheckBox getCheckBox();
	
	public abstract JButton getOkButton();
	
	public void disposeAndParse() {
		dispose();
	}
	
}
